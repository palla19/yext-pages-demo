{
  "name": "blog-react-studio-demo",
  "version": "0.0.0",
  "description": "An example repository to help users get started with React development on Yext Pages",
  "author": "sumo@yext.com",
  "license": "BSD-3-Clause",
  "type": "module",
  "scripts": {
    "dev": "pages dev",
    "prod": "pages prod",
    "studio": "npm run features && npm run localData && studio",
    "features": "pages generate features",
    "localData": "yext pages generate-test-data",
    "fmt": "prettier -w .",
    "lint": "eslint .",
    "clean": "rm -rf .artifact-output .yext/ dist/ localData sites-rendered-output",
    "generate-notices": "generate-license-file --input package.json --output ./THIRD-PARTY-NOTICES --overwrite",
    "build": "pages build"
  },
  "engines": {
    "node": "^18.0.0 || >=20.2.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@vitejs/plugin-react": "^4.2.1",
    "@yext/studio": "^0.25.0",
    "clsx": "^1.2.1",
    "cva": "npm:class-variance-authority@^0.7.0",
    "react": "^18.0.2",
    "react-dom": "^18.0.2",
    "react-markdown": "^8.0.7",
    "tailwind-merge": "^1.13.2",
    "vite": "^5.2.7"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.0.62",
    "@types/react-dom": "^18.0.20",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@yext/pages": "^1.1.0-beta.1",
    "autoprefixer": "^10.4.14",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "generate-license-file": "^1.3.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5"
  }
